<script type="text/javascript">
var menu_scrolling = false;
$(document).ready(function() {
//var config = {    
     //over: function(){ $('#menu-2599').css({'color': 'red'}); }, // function = onMouseOver callback (REQUIRED)    
//timeout: 500, // number = milliseconds delay before onMouseOut    
     //out: function(){} // function = onMouseOut callback (REQUIRED)    
//};
//$("#nav ul li ul li").hoverIntent(config);
$('#nav ul li ul li a').hover(
	function() { 
		$(this).parent('li').parent('ul').parent('.menuparent').children('a:first').addClass("redordead"); 
	},
	function() { 
		$(this).parent('li').parent('ul').parent('.menuparent').children('a:first').removeClass("redordead"); 
	}
);
	$(".menuparent").parents().each(function() {
	    var p = $(this);
	    var pos = p.css("position");
	    var idtag = $(this).attr('id');
	    var tagname = p.attr('tagName');
	    if (idtag.match(/(nice|menu)\-.+/) && (pos == "relative" || pos == "absolute" || pos == "fixed")) {
	       p.hover(function() { $(this).addClass("on-top"); }, function() { $(this).removeClass("on-top"); });
	    }
	});


var longmenus = [['.menu-path-publications ul:first', 1],
		['.menu-path-economic_updates ul:first', 1],
		['.menu-path-shop_country ul li.menu-path-shop_country ul:first', 2]];
var t1h =  parseInt($(window).height()) - 154;
for (var menu in longmenus) {
        var me = $(longmenus[menu][0]);
	var currh = me.height();
	if (currh > t1h + 10) {
		me.css({'overflow': 'hidden', 'height': t1h + 'px', 'width': '160px'});
        	var top_scroller = $('<div class="mnuup" style="display:none; width: ' + me.width() + 'px;"></div>');
        	var bot_scroller = $('<div class="mnudn" style="width: ' + me.width() + 'px;"></div>');
        	me.append(top_scroller);
        	me.append(bot_scroller);
        	top_scroller.bind('mouseenter', function() {
                	$(this).fadeOut(300);
			var tme = $(this).parent();
			var bs = tme.find('.mnudn');
                	tme.animate({ scrollTop: 0 }, 800, function() { 
                        	bs.fadeIn();
                	});
        	});
		if (longmenus[menu][1] != 42) {
        		bot_scroller.bind('mouseenter', function() {
                		$(this).fadeOut(300);
				var tme = $(this).parent();
				var ts = tme.find('.mnuup');
				var tmep = tme.parent();
                		tme.animate({ scrollTop: tme.find('li:last').offset().top - tme.height() - 100, easing: 'swing' }, 800, function() { 
                        		//ts.css({'top': (tmep.offset().top + tmep.height()) + 'px', 'left': tmep.offset().left + 'px'});
					if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)) {
                        			ts.css({'left': (tme.offset().left + 1) + 'px'});
					} else {
                        			ts.css({'left': tme.offset().left + 'px'});
					}
                        		ts.fadeIn();
                		});
        		});
		} else {
        		bot_scroller.bind('mouseenter', function() {
                		$(this).fadeOut(300);
                		var opts = { scrollTop: me.find('li:last').offset().top - me.height() - 100, easing: 'swing' };
                		me.animate(opts, 800, function() { 
                        		top_scroller.css({'left': (me.offset().left + 1) + 'px'});
                        		top_scroller.fadeIn();
                		});
        		});
		}	
	}
}
});
</script>

